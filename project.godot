; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="VGC Analyzer"
run/main_scene="res://Scene/UserInterface/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GlobalEnums="*res://Ressourcen/Global/global_enums.gd"
damage_calculation="*res://Ressourcen/Global/damage_calculation.gd"
AttackDescription="*res://Ressourcen/Global/attack_description.gd"

[display]

window/size/viewport_width=1728
window/size/viewport_height=972
window/size/initial_position_type=2
window/size/initial_screen=1
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
